{
    "version": 2,
    "buildCommand": "cd frontend && yarn install && yarn build && cd ../backend && pip install -r requirements.txt",
    "outputDirectory": "frontend/.next",
    "framework": "nextjs",
    "functions": {
      "backend/app/main.py": {
        "runtime": "vercel-python@3.12",
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/app/main.py"
      },
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "env": {
     
    }
  }